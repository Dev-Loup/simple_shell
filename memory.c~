#include "shell.h"

/**
 * check_malloc - checks if malloc was successful
 * @alloc: pointer to check
 * Return: 0 on success, 1 if it fails
 **/

int check_malloc(void *alloc)
{
	if (alloc == NULL)
		return (1);
	else
		return (0);
}

void free_garbage(char *path, char **cmd, int n)
{
	int i;

        if (n == 2)
        {
		free(path);
        }
        if (n == 3)
        {
		free(path);
		for (i = 0; cmd[i]; i++)
		{
			printf("free %s\n", cmd[i]);
			free(cmd[i]);
			cmd[i] = NULL;
		}
		free(cmd);
		cmd = NULL;
		printf("free %p\n", cmd);
        }
}
